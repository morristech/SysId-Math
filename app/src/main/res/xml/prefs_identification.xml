    xmlns:speedreader="http://schemas.android.com/apk/res-auto">






    <PreferenceCategory android:title="Parametric model">

        <ListPreference
            android:defaultValue="ARX"
            android:entries="@array/idPlantList"
            android:entryValues="@array/idPlantListValues"
            android:key="id1"
            android:summary="\t%s"
            android:title="Plant model type" />

        <ListPreference
            android:defaultValue="MA"
            android:entries="@array/idTimeseriesList"
            android:entryValues="@array/idTimeseriesListValues"
            android:key="id2"
            android:summary="\t%s"
            android:title="Timeseries model type" />

        <rafakob.multiedip.prefs.DoubleNumberPickerPreference
            android:defaultValue="2,1,2"
            android:key="id3"
            android:title="Structure"
            speedreader:labelFirst="@string/pref_lower"
            speedreader:labelSecond="@string/pref_upper" />


    </PreferenceCategory>





